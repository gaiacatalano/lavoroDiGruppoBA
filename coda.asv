classdef coda < handle

    properties
        lunghezzaMassima
        numeroUtentiInCoda
    end

    methods
        
        function coda = coda()
            coda.lunghezzaMassima = 1000;
        end

        function coda = aggiungiInCoda(coda)
            if coda.numeroUtentiInCoda < coda.lunghezzaMassima
                coda.numeroUtentiInCoda = coda.numeroUtentiInCoda+1;
            end
        end

    end
end